package triangle;

import java.io.BufferedReader;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.HashMap;

public class DegreeSort {
	
	public static void degreesort(String path) throws FileNotFoundException{
		HashMap <String, Integer> nodeDeg = new HashMap<String, Integer>();
        BufferedReader br = new BufferedReader(new FileReader(path));
        String line;
        while ((line = br.readLine()) != null) {
            String[] ids = line.split(" ");
            String a = ids[0];
            String b = ids[1];
            if(nodeDeg.containsKey(a)){
            	nodeDeg.put(a,nodeDeg.get(a)+1);
            }
            else{
            	nodeDeg.put(a,1);
            }
            if(nodeDeg.containsKey(b)){
            	nodeDeg.put(b,nodeDeg.get(b)+1);
            }
            else{
            	nodeDeg.put(b,1);
            }
            nodeDeg.put(a, nodeDeg.getOrDefault(a, 0) + 1);
            nodeDeg.put(b, nodeDeg.getOrDefault(b, 0) + 1);
        }
        System.out.println(nodeDeg.size());
	}
	
}
